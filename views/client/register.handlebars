<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8" />
    <title>Tonobil | Inscription</title>
    <link rel="stylesheet" href="/styleInscription.css" />
  </head>
  <body>
    <div class="center">
      <h1>S'inscrire</h1>
      <form method="POST" action="/client/register">
        <div class="pre">
          <div class="txt_field">
            <input type="text" name="nom" required />
            <span></span>
            <label>Nom </label>
          </div>
          <div class="txt_field">
            <input type="text" name="prenom" required />
            <span></span>
            <label>Pr√©nom</label>
          </div>
          <div class="txt_field">
            <input type="text" name="email" required />
            <span></span>
            <label>Email </label>
          </div>
          <div class="txt_field">
            <input type="password" name="password" required />
            <span></span>
            <label>Mot de Passe </label>
          </div>
        </div>
        <div class="txt_field">
          <input type="date" name="dateNaissance" required />
          <span></span>
          <label>Date de Naissance </label>
        </div>
        <div class="pre">
          <div class="txt_field">
            <input type="text" name="tel" required />
            <span></span>
            <label>Telephone </label>
          </div>
          <div class="txt_field">
            <input type="text" name="numPermis" required />
            <span></span>
            <label>Numero Permis </label>
          </div>

        </div>
        <div class="signup_link">
          Vous etes deja membere?
          <a href="#">Se Connecter</a>
        </div>

        <button class="submit" name="submit">S'Inscrire</button>
      </form>
    </div>
  </body>
</html>