<h1>Inscription Client</h1>
<form action="/client/register" method="post">
  <div>
    <label for="">Nom:
      <input id="" type="text" name="nom" />
    </label>
  </div>
  <div>
    <label for="">Prenom:
      <input id="" type="text" name="prenom" />
    </label>
  </div>
  <div>
    <label for="">Email:
      <input id="" type="text" name="email" />
    </label>
  </div>
  <div>
    <label for="">Password:
      <input type="" name="password" />
    </label>
  </div>

  <div>
    <label for="">Date de naissance:
      <input type="date" name="dateNaissance" />
    </label>
  </div>

  <div>
    <label for="">Telephone:
      <input type="text" name="tel" />
    </label>
  </div>
  <div>
    <label for="">Numero du permis:
      <input type="text" name="numPermis" />
    </label>
  </div>

  <input type="submit" value="se connecter" />
  <ul>
    {{#each errors}}
      <li>{{this.msg}}</li>
    {{/each}}
  </ul>
</form>